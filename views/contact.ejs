<body>

<br><br>
	<div class="container-contact100">
		<div class="wrap-contact100">
			<form class="contact100-form validate-form">
				<span class="contact100-form-title" style="font-family: 'Quicksand', sans-serif;"><u>
                    Send Us A Message</u>
				</span>

				<label class="label-input100" for="first-name"  style="margin-top: 0px; border-color: transparent; font-family: 'Quicksand', sans-serif;">Tell us your name *</label>
				<div class="wrap-input100 rs1-wrap-input100 validate-input" data-validate="Type first name">
					<input id="first-name" class="input100" type="text" name="first-name" placeholder="First name">
					<span class="focus-input100"></span>
				</div>
				<div class="wrap-input100 rs2-wrap-input100 validate-input" data-validate="Type last name">
					<input class="input100" type="text" name="last-name" placeholder="Last name">
					<span class="focus-input100"></span>
				</div>

				<label class="label-input100" for="email" style="margin-top: 0px; border-color: transparent; font-family: 'Quicksand', sans-serif;">Enter your email *</label>
				<div class="wrap-input100 validate-input" data-validate = "Valid email is required: ex@abc.xyz">
					<input id="email" class="input100" type="text" name="email" placeholder="Eg. example@email.com">
					<span class="focus-input100"></span>
				</div>

				<label class="label-input100" for="phone" style="margin-top: 0px; border-color: transparent; font-family: 'Quicksand', sans-serif;">Enter phone number</label>
				<div class="wrap-input100">
					<input id="phone" class="input100" type="text" name="phone" placeholder="Eg. +91 00000 00000">
					<span class="focus-input100"></span>
				</div>

				<label class="label-input100" for="message" style="margin-top: 0px; border-color: transparent; font-family: 'Quicksand', sans-serif;">Message *</label>
				<div class="wrap-input100 validate-input" data-validate = "Message is required">
					<textarea id="message" class="input100" name="message" placeholder="Write us a message"></textarea>
					<span class="focus-input100"></span>
				</div>

				<div class="container-contact100-form-btn">
					<button class="contact100-form-btn" style="font-family: 'Quicksand', sans-serif;">
						Send Message
					</button>
				</div>
			</form>

			<div class="contact100-more flex-col-c-m" style="background-image: url('/bg-01.jpg');">
				<div class="flex-w size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-map-marker"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20" style="font-family: 'Quicksand', sans-serif;">
							Address
						</span>

						<span class="txt2" style="font-family: 'Quicksand', sans-serif;">E-407
                            CSE Department
                            <br>
                            KIET Group of Institutions
						</span>
					</div>
				</div>

				<div class="dis-flex size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-phone-handset"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20" style="font-family: 'Quicksand', sans-serif;">
							Lets Talk
						</span>

						<span class="txt3" style="font-family: 'Quicksand', sans-serif;">
                            Aakash<br>
                            +91 87914 31819
                            <br><br>
                            Ritwick Bhargav
                            <br>+91 82182 90235
						</span>
					</div>
				</div>

				<div class="dis-flex size1 p-b-47">
					<div class="txt1 p-r-25">
						<span class="lnr lnr-envelope"></span>
					</div>

					<div class="flex-col size2">
						<span class="txt1 p-b-20" style="font-family: 'Quicksand', sans-serif;">
							General Support
						</span>

						<span class="txt3">
							dsckiet@gmail.com
						</span>
					</div>
				</div>
			</div>
		</div>
	</div>



